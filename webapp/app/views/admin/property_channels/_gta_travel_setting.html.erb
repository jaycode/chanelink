<% if !@property_channel.gta_travel_property_id.blank? %>
  <pre>
    <% contract_xml = GtaTravelChannel.get_property_contract(@property_channel.gta_travel_property_id) %>
    <% doc = Nokogiri.XML(contract_xml) do |config| %>
      <% config.default_xml.noblanks %>
    <% end %>
    <%= doc.to_xml(:indent => 2) %>
  </pre>
<% end %>
<p>
  <label><%= t('property_channels.new.label.property_id') %><%= required %></label>
  <%= f.text_field :gta_travel_property_id %>
</p>
<p>
  <label><%= t('property_channels.new.label.contract_id') %><%= required %></label>
  <%= f.text_field :gta_travel_contract_id %>
</p>
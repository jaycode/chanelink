<%= page_title t('admin.accounts.new.label.heading') %>
<div id="newAccount">
  <div class="col9">
    <h2><%= raw t('admin.accounts.new.label.heading') %></h2>
    <%= render 'breadcrumb' %>
    <%= form_for [@account, @super_member], :url => {:controller => 'admin/accounts', :action => 'create'} do |f| %>
      <p>
        <label><%= t('members.new.label.name') %><%= required %></label>
        <%= f.text_field :name %>
      </p>
      <p>
        <label><%= t('members.new.label.email') %><%= required %></label>
        <%= f.text_field :email %>
      </p>
      <br/>
      <p><%= f.submit t("button.back"), :name => "back_button" %><%= f.submit t("button.continue") %></p>
      <p></p>
    <% end %>
  </div>
</div>